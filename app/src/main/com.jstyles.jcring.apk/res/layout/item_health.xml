<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:id="@+id/health_tab_mian"
        android:layout_width="0dp"
        android:layout_height="0dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintDimensionRatio="h,169:200"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent">
        <TextView
            android:textSize="@dimen/sp_14"
            android:textColor="@android:color/black"
            android:id="@+id/title_name_one"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_18"
            android:maxLines="1"
            android:includeFontPadding="false"
            android:layout_marginStart="@dimen/dp_21"/>
        <TextView
            android:textSize="@dimen/sp_12"
            android:textColor="#434343"
            android:id="@+id/title_name_two"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_14"
            android:maxLines="3"
            android:includeFontPadding="false"
            android:layout_below="@+id/title_name_one"
            android:layout_marginStart="@dimen/dp_21"/>
        <TextView
            android:textSize="@dimen/sp_12"
            android:textColor="#434343"
            android:id="@+id/title_name_there"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_6_5"
            android:maxLines="3"
            android:includeFontPadding="false"
            android:layout_below="@+id/title_name_two"
            android:layout_marginStart="@dimen/dp_21"
            android:layout_marginEnd="@dimen/dp_21"/>
        <LinearLayout
            android:layout_gravity="center"
            android:orientation="horizontal"
            android:id="@+id/value_rt"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_above="@+id/tips_celiang"
            android:layout_centerHorizontal="true">
            <com.jstyles.jcring.views.public_views.MultiplTextView
                android:textSize="@dimen/sp_20"
                android:textColor="#434343"
                android:layout_gravity="bottom"
                android:id="@+id/tips_value"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="0"
                android:maxLines="1"
                app:removeDefaultPadding="true"/>
            <com.jstyles.jcring.views.public_views.MultiplTextView
                android:textSize="@dimen/sp_10"
                android:textColor="#434343"
                android:layout_gravity="bottom"
                android:id="@+id/tips"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="@string/mmol"
                android:layout_marginStart="@dimen/dp_6"
                app:removeDefaultPadding="true"/>
        </LinearLayout>
        <com.jstyles.jcring.views.public_views.MultiplTextView
            android:textSize="@dimen/sp_11"
            android:textColor="#434343"
            android:id="@+id/tips_celiang"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_9"
            android:layout_marginBottom="@dimen/dp_32"
            android:text="dddfdfdd"
            android:maxLines="1"
            android:layout_alignParentBottom="true"
            android:layout_centerHorizontal="true"
            app:removeDefaultPadding="true"/>
    </RelativeLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
