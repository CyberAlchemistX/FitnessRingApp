<?xml version="1.0" encoding="utf-8"?>
<com.jstyles.jcring.views.ecg_stick_1791.MyNestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/nestedScrollView"
    android:background="@color/main_bg_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:keepScreenOn="true">
        <RelativeLayout
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_41"
            android:layout_marginTop="@dimen/dp_20_5">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/last_img"
                android:background="@drawable/ripple_bg"
                android:padding="@dimen/dp_9"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_31"
                android:src="@mipmap/last_img_fill"
                android:adjustViewBounds="true"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_27"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/nest_img"
                android:background="@drawable/ripple_bg"
                android:padding="@dimen/dp_9"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_31"
                android:src="@mipmap/next_img_fill"
                android:adjustViewBounds="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_27"
                android:layout_alignParentEnd="true"/>
            <TextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:id="@+id/date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <com.jstyles.jcring.views.FontMiSansMediumView
            android:textSize="@dimen/sp_23"
            android:textColor="@color/main_text_color"
            android:gravity="center"
            android:id="@+id/TEMP_value"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_40"
            android:text="0"
            android:includeFontPadding="false"
            android:layout_below="@+id/title"
            android:layout_centerHorizontal="true"/>
        <com.jstyles.jcring.views.public_views.MultiplTextView
            android:textSize="@dimen/sp_15"
            android:textColor="@color/main_text_color"
            android:gravity="center"
            android:id="@+id/heart_value_tips"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_7"
            android:text="@string/bpm"
            android:includeFontPadding="false"
            android:layout_below="@+id/TEMP_value"
            android:layout_centerHorizontal="true"
            app:removeDefaultPadding="true"/>
        <RelativeLayout
            android:id="@+id/index_date_rt"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_below="@+id/heart_value_tips">
            <com.jstyles.jcring.views.public_views.MultiplTextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:gravity="center"
                android:id="@+id/index_date"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="--"
                android:includeFontPadding="false"
                android:layout_centerVertical="true"
                app:removeDefaultPadding="true"/>
        </RelativeLayout>
        <com.jstyles.jcring.views.ecg_stick_1791.Vo2MaxWeekView
            android:id="@+id/temp_dayview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_240"
            android:layout_marginTop="@dimen/dp_7"
            android:layout_below="@+id/index_date_rt"/>
        <RelativeLayout
            android:id="@+id/vo2max_last_rel"
            android:background="@drawable/bg_rect_msg_item_r20"
            android:padding="@dimen/dp_15"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_264"
            android:layout_marginBottom="@dimen/dp_15"
            android:layout_below="@+id/temp_dayview"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginEnd="@dimen/dp_15"
            android:elevation="@dimen/dp_1">
            <TextView
                android:textColor="@color/msg_remark_text_color"
                android:gravity="center_vertical"
                android:id="@+id/vo2max_week_fr_tv_date"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_21"
                android:text=""/>
            <ImageView
                android:id="@+id/vo2max_about"
                android:layout_width="@dimen/dp_21"
                android:layout_height="@dimen/dp_21"
                android:src="@mipmap/tan_1"
                android:layout_alignParentRight="true"/>
            <View
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1"
                android:layout_marginTop="@dimen/dp_30"
                style="@style/linemsgsetting"/>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/dp_45">
                <LinearLayout
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <TextView
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/main_text_color"
                        android:gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/vo2max_title"/>
                    <RelativeLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_17_5">
                        <com.jstyles.jcring.views.Vo2MaxProgressView
                            android:id="@+id/vo2max_fr_progress_view"
                            android:layout_width="@dimen/dp_132"
                            android:layout_height="@dimen/dp_125"
                            android:layout_centerInParent="true"/>
                        <TextView
                            android:textColor="@color/white"
                            android:gravity="center"
                            android:id="@+id/vo2max_fr_tv_view"
                            android:background="@drawable/bg_e5e5e5_r13"
                            android:layout_width="@dimen/dp_79"
                            android:layout_height="@dimen/dp_26"
                            android:layout_marginTop="@dimen/dp_m_10"
                            android:text="Excellent"
                            android:layout_below="@+id/vo2max_fr_progress_view"
                            android:layout_centerHorizontal="true"/>
                    </RelativeLayout>
                </LinearLayout>
                <LinearLayout
                    android:gravity="center_horizontal"
                    android:orientation="vertical"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <TextView
                        android:textSize="@dimen/sp_16"
                        android:textColor="@color/main_text_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/vo2_max_age"/>
                    <TextView
                        android:textSize="@dimen/sp_60"
                        android:textColor="#803a97"
                        android:id="@+id/vo2max_week_fr_tv_age"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_11"
                        android:text="--"/>
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:textColor="@color/msg_remark_text_color"
                        android:gravity="center_horizontal"
                        android:id="@+id/vo2max_week_fr_tv_age_tip"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="--"/>
                </LinearLayout>
            </LinearLayout>
        </RelativeLayout>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/vo2max_rv_data"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/vo2max_last_rel"/>
    </RelativeLayout>
</com.jstyles.jcring.views.ecg_stick_1791.MyNestedScrollView>
