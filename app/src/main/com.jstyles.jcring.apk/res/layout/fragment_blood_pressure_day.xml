<?xml version="1.0" encoding="utf-8"?>
<com.jstyles.jcring.views.ecg_stick_1791.MyNestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/nestedScrollView"
    android:background="@color/main_bg_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:keepScreenOn="true">
        <RelativeLayout
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_41"
            android:layout_marginTop="@dimen/dp_20_5">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/last_img"
                android:background="@drawable/ripple_bg"
                android:padding="@dimen/dp_9"
                android:layout_width="@dimen/dp_41"
                android:layout_height="@dimen/dp_41"
                android:src="@mipmap/last_img"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_27"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/nest_img"
                android:background="@drawable/ripple_bg"
                android:padding="@dimen/dp_9"
                android:layout_width="@dimen/dp_41"
                android:layout_height="@dimen/dp_41"
                android:src="@mipmap/nest_img"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_27"
                android:layout_alignParentEnd="true"/>
            <TextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:id="@+id/date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <LinearLayout
            android:gravity="center"
            android:id="@+id/title_rt"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_30"
            android:layout_below="@+id/title">
            <com.jstyles.jcring.views.public_views.MultiplTextView
                android:textSize="@dimen/sp_34"
                android:textStyle="bold"
                android:textColor="@color/main_text_color"
                android:gravity="center"
                android:id="@+id/TEMP_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                app:removeDefaultPadding="true"
                app:textFont="fonts/DINCond-Bold.otf"/>
        </LinearLayout>
        <com.jstyles.jcring.views.public_views.MultiplTextView
            android:textSize="@dimen/sp_15"
            android:textColor="@color/main_text_color"
            android:gravity="center"
            android:id="@+id/heart_value_tips"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_7"
            android:text="@string/mmhg"
            android:includeFontPadding="false"
            android:layout_below="@+id/title_rt"
            android:layout_centerHorizontal="true"
            app:removeDefaultPadding="true"/>
        <RelativeLayout
            android:id="@+id/index_date_rt"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_below="@+id/heart_value_tips">
            <com.jstyles.jcring.views.public_views.MultiplTextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:gravity="center"
                android:id="@+id/index_date"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="00"
                android:includeFontPadding="false"
                android:layout_centerInParent="true"
                android:layout_centerHorizontal="true"
                app:removeDefaultPadding="true"/>
        </RelativeLayout>
        <com.jstyles.jcring.views.oximeter_1963.BloodPressure_dayView
            android:id="@+id/temp_dayview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_260"
            android:layout_marginTop="@dimen/dp_7"
            android:layout_below="@+id/index_date_rt"/>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/other_lin1"
            android:background="@drawable/bg_rect_msg_item_r20_ripple"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_83"
            android:layout_below="@+id/temp_dayview"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginEnd="@dimen/dp_15">
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textSize="@dimen/sp_21"
                    android:textColor="@color/main_text_color"
                    android:id="@+id/day_avg_temp_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="70"/>
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:textColor="@color/msg_remark_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="@string/day_avg_bloodpressure"/>
            </LinearLayout>
            <LinearLayout
                android:background="@color/home_detail_line_bg"
                android:layout_width="@dimen/dp_0_5"
                android:layout_height="@dimen/dp_40"/>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textSize="@dimen/sp_21"
                    android:textColor="@color/main_text_color"
                    android:id="@+id/day_min_temp_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="70"/>
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:textColor="@color/msg_remark_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="@string/day_min_bloodpressure"/>
            </LinearLayout>
            <LinearLayout
                android:background="@color/home_detail_line_bg"
                android:layout_width="@dimen/dp_0_5"
                android:layout_height="@dimen/dp_40"/>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textSize="@dimen/sp_21"
                    android:textColor="@color/main_text_color"
                    android:id="@+id/day_max_temp_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="70"/>
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:textColor="@color/msg_remark_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="@string/day_max_bloodpressure"/>
            </LinearLayout>
        </LinearLayout>
        <RelativeLayout
            android:background="@drawable/bg_rect_msg_item_r20_ripple"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_55"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_below="@+id/other_lin1"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginEnd="@dimen/dp_15">
            <TextView
                android:textSize="@dimen/sp_15"
                android:textColor="@color/main_text_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/oxy_day_bloodpressure"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_15"/>
            <TextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/main_text_color"
                android:id="@+id/oxy_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="10æ¬¡"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_26"
                android:layout_alignParentEnd="true"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/lest_img"
                android:adjustViewBounds="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_15"
                android:layout_alignParentEnd="true"/>
        </RelativeLayout>
    </RelativeLayout>
</com.jstyles.jcring.views.ecg_stick_1791.MyNestedScrollView>
