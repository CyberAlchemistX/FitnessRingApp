<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/main"
    android:background="@color/main_bg_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/layout_title"/>
    <androidx.core.widget.NestedScrollView
        android:orientation="vertical"
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">
            <RelativeLayout
                android:id="@+id/setting_new4_rel_auto"
                android:background="@drawable/bg_rect_msg_item_r20_ripple"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_60"
                android:layout_marginTop="@dimen/dp_12"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginEnd="@dimen/dp_15"
                android:elevation="@dimen/dp_1">
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/setting_new4_iv_auto"
                    android:layout_width="@dimen/dp_22"
                    android:layout_height="@dimen/dp_22"
                    android:src="@mipmap/setting_new4_auto_icon"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_20"/>
                <TextView
                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/main_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/device_meun_0"
                    android:includeFontPadding="false"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_13"
                    android:layout_toEndOf="@+id/setting_new4_iv_auto"/>
                <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="@dimen/dp_6"
                    android:layout_height="@dimen/dp_11"
                    android:src="@mipmap/lest_img"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_28_5"
                    android:layout_alignParentEnd="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/setting_new4_iv_autosport_rt"
                android:background="@drawable/bg_rect_msg_item_r20_ripple"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_60"
                android:layout_marginTop="@dimen/dp_12"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginEnd="@dimen/dp_15"
                android:elevation="@dimen/dp_1">
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/setting_new4_iv_autosport"
                    android:layout_width="@dimen/dp_22"
                    android:layout_height="@dimen/dp_22"
                    android:src="@mipmap/setting_new4_autosport_icon"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_20"/>
                <TextView
                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/main_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/automatic_autosport_monitoring"
                    android:includeFontPadding="false"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_13"
                    android:layout_toEndOf="@+id/setting_new4_iv_autosport"/>
                <com.jstyles.jcring.views.ecg_stick_1791.SwitchButton
                    android:id="@+id/auto_switch_sport"
                    android:layout_width="@dimen/dp_50"
                    android:layout_height="@dimen/dp_25"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_20"
                    android:layout_alignParentEnd="true"
                    app:animate="true"
                    app:borderWidth="@dimen/dp_0_3"
                    app:offBorderColor="@color/black"
                    app:offColor="@color/black"
                    app:onColor="@color/btn_bg"
                    app:spotColor="@android:color/white"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/setting_new4_rel_target"
                android:background="@drawable/bg_rect_msg_item_r20_ripple"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_60"
                android:layout_marginTop="@dimen/dp_12"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginEnd="@dimen/dp_15"
                android:elevation="@dimen/dp_1">
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/setting_new4_iv_target"
                    android:layout_width="@dimen/dp_22"
                    android:layout_height="@dimen/dp_22"
                    android:src="@mipmap/setting_new4_target_icon"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_20"/>
                <TextView
                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/main_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/device_meun_1"
                    android:includeFontPadding="false"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_13"
                    android:layout_toEndOf="@+id/setting_new4_iv_target"/>
                <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="@dimen/dp_6"
                    android:layout_height="@dimen/dp_11"
                    android:src="@mipmap/lest_img"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_28_5"
                    android:layout_alignParentEnd="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/setting_new4_rel_reset"
                android:background="@drawable/bg_rect_msg_item_r20_ripple"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_60"
                android:layout_marginTop="@dimen/dp_12"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginEnd="@dimen/dp_15"
                android:elevation="@dimen/dp_1">
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/setting_new4_iv_reset"
                    android:layout_width="@dimen/dp_22"
                    android:layout_height="@dimen/dp_22"
                    android:src="@mipmap/setting_new4_reset_icon"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_20"/>
                <TextView
                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/main_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/devices_reset"
                    android:includeFontPadding="false"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_13"
                    android:layout_toEndOf="@+id/setting_new4_iv_reset"/>
                <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="@dimen/dp_6"
                    android:layout_height="@dimen/dp_11"
                    android:src="@mipmap/lest_img"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_28_5"
                    android:layout_alignParentEnd="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/lighting_indicator_rt"
                android:background="@drawable/bg_rect_msg_item_r20_ripple"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_60"
                android:layout_marginTop="@dimen/dp_12"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginEnd="@dimen/dp_15"
                android:elevation="@dimen/dp_1">
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/setting_new4_lighting_indicator_icon"
                    android:layout_width="@dimen/dp_22"
                    android:layout_height="@dimen/dp_22"
                    android:src="@mipmap/setting_new4_lighting_indicator_icon"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_20"/>
                <TextView
                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/main_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/lighting_indicator"
                    android:includeFontPadding="false"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_13"
                    android:layout_toEndOf="@+id/setting_new4_lighting_indicator_icon"/>
                <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="@dimen/dp_6"
                    android:layout_height="@dimen/dp_11"
                    android:src="@mipmap/lest_img"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_28_5"
                    android:layout_alignParentEnd="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/tounch_rt"
                android:background="@drawable/bg_rect_msg_item_r20_ripple"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_60"
                android:layout_marginTop="@dimen/dp_12"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginEnd="@dimen/dp_15"
                android:elevation="@dimen/dp_1">
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/setting_new4_tounch_icon"
                    android:layout_width="@dimen/dp_22"
                    android:layout_height="@dimen/dp_22"
                    android:src="@mipmap/setting_new4_tounch_icon"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_20"/>
                <TextView
                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/main_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/touch_mode"
                    android:includeFontPadding="false"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_13"
                    android:layout_toEndOf="@+id/setting_new4_tounch_icon"/>
                <androidx.appcompat.widget.AppCompatImageView
                    android:layout_width="@dimen/dp_6"
                    android:layout_height="@dimen/dp_11"
                    android:src="@mipmap/lest_img"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_28_5"
                    android:layout_alignParentEnd="true"/>
            </RelativeLayout>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</LinearLayout>
