<?xml version="1.0" encoding="utf-8"?>
<com.jstyles.jcring.views.ecg_stick_1791.MyNestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/nestedScrollView"
    android:background="@color/main_bg_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:keepScreenOn="true">
        <RelativeLayout
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_41"
            android:layout_marginTop="@dimen/dp_20_5">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/last_img"
                android:background="@drawable/ripple_bg"
                android:padding="@dimen/dp_9"
                android:layout_width="@dimen/dp_41"
                android:layout_height="@dimen/dp_41"
                android:src="@mipmap/last_img"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_27"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/nest_img"
                android:background="@drawable/ripple_bg"
                android:padding="@dimen/dp_9"
                android:layout_width="@dimen/dp_41"
                android:layout_height="@dimen/dp_41"
                android:src="@mipmap/nest_img"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_27"
                android:layout_alignParentEnd="true"/>
            <TextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:id="@+id/date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <LinearLayout
            android:gravity="center"
            android:id="@+id/title_rt"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_30"
            android:layout_below="@+id/title">
            <com.jstyles.jcring.views.public_views.MultiplTextView
                android:textSize="@dimen/sp_34"
                android:textStyle="bold"
                android:textColor="@color/main_text_color"
                android:gravity="center"
                android:id="@+id/temp_monthview_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                app:removeDefaultPadding="true"
                app:textFont="fonts/DINCond-Bold.otf"/>
        </LinearLayout>
        <com.jstyles.jcring.views.public_views.MultiplTextView
            android:textSize="@dimen/sp_15"
            android:textColor="@color/main_text_color"
            android:gravity="center"
            android:id="@+id/heart_value_tips"
            android:visibility="gone"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_7"
            android:text="@string/mmhg"
            android:includeFontPadding="false"
            android:layout_below="@+id/title_rt"
            android:layout_centerHorizontal="true"
            app:removeDefaultPadding="true"/>
        <RelativeLayout
            android:id="@+id/index_date_rt"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_below="@+id/heart_value_tips">
            <com.jstyles.jcring.views.public_views.MultiplTextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:gravity="center"
                android:id="@+id/index_date"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_10"
                android:text=""
                android:includeFontPadding="false"
                android:layout_centerHorizontal="true"
                app:removeDefaultPadding="true"/>
        </RelativeLayout>
        <com.jstyles.jcring.views.oximeter_1963.BloodPressure_monthView
            android:id="@+id/temp_monthview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_240"
            android:layout_marginTop="@dimen/dp_7"
            android:layout_below="@+id/index_date_rt"/>
        <RelativeLayout
            android:id="@+id/Pie_rt"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_160"
            android:layout_marginTop="@dimen/dp_5"
            android:layout_below="@+id/temp_monthview">
            <com.jstyles.jcring.views.oximeter_1963.PieChartView
                android:id="@+id/percentPieView"
                android:background="@android:color/transparent"
                android:layout_width="@dimen/dp_130"
                android:layout_height="@dimen/dp_130"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_32"
                app:centerTextColor="@color/msg_remark_text_color"
                app:centerTextSize="@dimen/sp_11"
                app:circleWidth="@dimen/dp_25"
                app:circletextvalue="@string/bloodpressure_status_Tips"
                app:dataTextColor="@color/main_text_color"/>
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/dp_10"
                android:layout_alignTop="@+id/percentPieView"
                android:layout_alignBottom="@+id/percentPieView"
                android:layout_marginStart="@dimen/dp_50"
                android:layout_toEndOf="@+id/percentPieView">
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <ImageView
                        android:id="@+id/bg_rect_normal"
                        android:layout_width="@dimen/dp_10"
                        android:layout_height="@dimen/dp_10"
                        android:src="@drawable/bg_rect_normal"
                        android:scaleType="fitXY"
                        android:layout_alignParentBottom="true"/>
                    <com.jstyles.jcring.views.public_views.MultiplTextView
                        android:textSize="@dimen/sp_11"
                        android:textColor="@color/msg_remark_text_color"
                        android:gravity="center"
                        android:id="@+id/normal_data"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/normal_data"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_toEndOf="@+id/bg_rect_normal"
                        app:removeDefaultPadding="true"/>
                    <com.jstyles.jcring.views.public_views.MultiplTextView
                        android:textSize="@dimen/sp_23"
                        android:textColor="@color/main_text_color"
                        android:gravity="center"
                        android:id="@+id/normal_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_toEndOf="@+id/normal_data"
                        app:removeDefaultPadding="true"
                        app:textFont="fonts/DINCond-Bold.otf"/>
                    <com.jstyles.jcring.views.public_views.MultiplTextView
                        android:textSize="@dimen/sp_10"
                        android:textColor="@color/main_text_color"
                        android:gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/ci"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_toEndOf="@+id/normal_value"
                        app:removeDefaultPadding="true"
                        app:textFont="fonts/DINCond-Bold.otf"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <ImageView
                        android:id="@+id/bg_rect_low"
                        android:layout_width="@dimen/dp_10"
                        android:layout_height="@dimen/dp_10"
                        android:src="@drawable/bg_rect_low"
                        android:scaleType="fitXY"
                        android:layout_alignParentBottom="true"/>
                    <com.jstyles.jcring.views.public_views.MultiplTextView
                        android:textSize="@dimen/sp_11"
                        android:textColor="@color/msg_remark_text_color"
                        android:gravity="center"
                        android:id="@+id/low_data"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/low_side"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_toEndOf="@+id/bg_rect_low"
                        app:removeDefaultPadding="true"/>
                    <com.jstyles.jcring.views.public_views.MultiplTextView
                        android:textSize="@dimen/sp_23"
                        android:textColor="@color/main_text_color"
                        android:gravity="center"
                        android:id="@+id/low_data_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_toEndOf="@+id/low_data"
                        app:removeDefaultPadding="true"
                        app:textFont="fonts/DINCond-Bold.otf"/>
                    <com.jstyles.jcring.views.public_views.MultiplTextView
                        android:textSize="@dimen/sp_10"
                        android:textColor="@color/main_text_color"
                        android:gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/ci"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_toEndOf="@+id/low_data_value"
                        app:removeDefaultPadding="true"
                        app:textFont="fonts/DINCond-Bold.otf"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <ImageView
                        android:id="@+id/bg_rect_height"
                        android:layout_width="@dimen/dp_10"
                        android:layout_height="@dimen/dp_10"
                        android:src="@drawable/bg_rect_height"
                        android:scaleType="fitXY"
                        android:layout_alignParentBottom="true"/>
                    <com.jstyles.jcring.views.public_views.MultiplTextView
                        android:textSize="@dimen/sp_11"
                        android:textColor="@color/msg_remark_text_color"
                        android:gravity="center"
                        android:id="@+id/high_side"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/high_side"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_toEndOf="@+id/bg_rect_height"
                        app:removeDefaultPadding="true"/>
                    <com.jstyles.jcring.views.public_views.MultiplTextView
                        android:textSize="@dimen/sp_23"
                        android:textColor="@color/main_text_color"
                        android:gravity="center"
                        android:id="@+id/high_data_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_toEndOf="@+id/high_side"
                        app:removeDefaultPadding="true"
                        app:textFont="fonts/DINCond-Bold.otf"/>
                    <com.jstyles.jcring.views.public_views.MultiplTextView
                        android:textSize="@dimen/sp_10"
                        android:textColor="@color/main_text_color"
                        android:gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/ci"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_toEndOf="@+id/high_data_value"
                        app:removeDefaultPadding="true"
                        app:textFont="fonts/DINCond-Bold.otf"/>
                </RelativeLayout>
                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <ImageView
                        android:id="@+id/bg_rect_error"
                        android:layout_width="@dimen/dp_10"
                        android:layout_height="@dimen/dp_10"
                        android:src="@drawable/bg_rect_error"
                        android:scaleType="fitXY"
                        android:layout_alignParentBottom="true"/>
                    <com.jstyles.jcring.views.public_views.MultiplTextView
                        android:textSize="@dimen/sp_11"
                        android:textColor="@color/msg_remark_text_color"
                        android:gravity="center"
                        android:id="@+id/abnormal_data_maicha"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/abnormal_data_maicha"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_toEndOf="@+id/bg_rect_error"
                        app:removeDefaultPadding="true"/>
                    <com.jstyles.jcring.views.public_views.MultiplTextView
                        android:textSize="@dimen/sp_23"
                        android:textColor="@color/main_text_color"
                        android:gravity="center"
                        android:id="@+id/error_data_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="@dimen/dp_10"
                        android:layout_toEndOf="@+id/abnormal_data_maicha"
                        app:removeDefaultPadding="true"
                        app:textFont="fonts/DINCond-Bold.otf"/>
                    <com.jstyles.jcring.views.public_views.MultiplTextView
                        android:textSize="@dimen/sp_10"
                        android:textColor="@color/main_text_color"
                        android:gravity="center"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/ci"
                        android:layout_alignParentBottom="true"
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_toEndOf="@+id/error_data_value"
                        app:removeDefaultPadding="true"
                        app:textFont="fonts/DINCond-Bold.otf"/>
                </RelativeLayout>
            </LinearLayout>
        </RelativeLayout>
        <LinearLayout
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:id="@+id/other_lin1"
            android:background="@drawable/bg_rect_msg_item_r20_ripple"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_83"
            android:layout_below="@+id/Pie_rt"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginEnd="@dimen/dp_15">
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textSize="@dimen/sp_21"
                    android:textColor="@color/main_text_color"
                    android:id="@+id/month_avg_temp_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"/>
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:textColor="@color/msg_remark_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="@string/month_avg_bloodpressure"/>
            </LinearLayout>
            <LinearLayout
                android:background="@color/home_detail_line_bg"
                android:layout_width="@dimen/dp_0_5"
                android:layout_height="@dimen/dp_40"/>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textSize="@dimen/sp_21"
                    android:textColor="@color/main_text_color"
                    android:id="@+id/month_min_temp_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"/>
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:textColor="@color/msg_remark_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="@string/month_min_bloodpressure"/>
            </LinearLayout>
            <LinearLayout
                android:background="@color/home_detail_line_bg"
                android:layout_width="@dimen/dp_0_5"
                android:layout_height="@dimen/dp_40"/>
            <LinearLayout
                android:gravity="center"
                android:orientation="vertical"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1">
                <TextView
                    android:textSize="@dimen/sp_21"
                    android:textColor="@color/main_text_color"
                    android:id="@+id/month_max_temp_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="0"/>
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:textColor="@color/msg_remark_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="@dimen/dp_10"
                    android:text="@string/month_max_bloodpressure"/>
            </LinearLayout>
        </LinearLayout>
        <RelativeLayout
            android:background="@drawable/bg_rect_msg_item_r20_ripple"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_55"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_marginBottom="@dimen/dp_10"
            android:layout_below="@+id/other_lin1"
            android:layout_marginStart="@dimen/dp_15"
            android:layout_marginEnd="@dimen/dp_15">
            <TextView
                android:textSize="@dimen/sp_15"
                android:textColor="@color/main_text_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/oxy_month_bloodpressure"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_15"/>
            <TextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/main_text_color"
                android:id="@+id/oxy_status"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="--"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_26"
                android:layout_alignParentEnd="true"/>
            <ImageView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@mipmap/lest_img"
                android:adjustViewBounds="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_15"
                android:layout_alignParentEnd="true"/>
        </RelativeLayout>
    </RelativeLayout>
</com.jstyles.jcring.views.ecg_stick_1791.MyNestedScrollView>
