<?xml version="1.0" encoding="utf-8"?>
<com.jstyles.jcring.views.ecg_stick_1791.MyNestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/nestedScrollView"
    android:background="@color/main_bg_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:keepScreenOn="true">
        <RelativeLayout
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_41"
            android:layout_marginTop="@dimen/dp_20_5">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/last_img"
                android:background="@drawable/ripple_bg"
                android:padding="@dimen/dp_9"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_31"
                android:src="@mipmap/last_img_fill"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_27"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/nest_img"
                android:background="@drawable/ripple_bg"
                android:padding="@dimen/dp_9"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_31"
                android:src="@mipmap/next_img_fill"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_27"
                android:layout_alignParentEnd="true"/>
            <TextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:id="@+id/date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <LinearLayout
            android:gravity="center"
            android:id="@+id/title_rt"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_30"
            android:layout_below="@+id/title">
            <com.jstyles.jcring.views.FontMiSansMediumView
                android:textSize="@dimen/sp_23"
                android:textColor="@color/main_text_color"
                android:gravity="center"
                android:id="@+id/TEMP_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="0"
                android:includeFontPadding="false"
                app:removeDefaultPadding="true"/>
            <com.jstyles.jcring.views.public_views.MultiplTextView
                android:textSize="@dimen/sp_23"
                android:textColor="@color/main_text_color"
                android:gravity="center"
                android:id="@+id/heart_value_tips"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/baifenbi"
                android:includeFontPadding="false"
                app:removeDefaultPadding="true"/>
        </LinearLayout>
        <RelativeLayout
            android:id="@+id/index_date_rt"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_below="@+id/title_rt">
            <com.jstyles.jcring.views.public_views.MultiplTextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:gravity="center"
                android:id="@+id/index_date"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:includeFontPadding="false"
                android:layout_centerInParent="true"
                android:layout_centerHorizontal="true"
                app:removeDefaultPadding="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/Oxy_rt"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_260"
            android:layout_below="@+id/index_date_rt">
            <com.jstyles.jcring.views.oximeter_1963.Oxy_dayView
                android:id="@+id/temp_dayview"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/dp_7"/>
            <com.jstyles.jcring.views.oximeter_1963.Oxy_dayMDView
                android:id="@+id/temp_dayMDview"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/dp_7"/>
        </RelativeLayout>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginTop="@dimen/dp_m_27_5"
            android:layout_below="@+id/Oxy_rt"
            android:layout_alignParentBottom="true">
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:visibility="visible"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_38">
                <com.jstyles.jcring.views.public_views.CircleColor
                    android:layout_width="@dimen/dp_9"
                    android:layout_height="@dimen/dp_9"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_5"
                    android:layout_toStartOf="@+id/abnormal_value"
                    app:round_circleColor="#f16c6c"/>
                <androidx.appcompat.widget.AppCompatTextView
                    android:textSize="@dimen/sp_11"
                    android:textColor="@color/main_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/spo2_jujian3_value"
                    android:includeFontPadding="false"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_17"/>
                <com.jstyles.jcring.views.public_views.CircleColor
                    android:layout_width="@dimen/dp_9"
                    android:layout_height="@dimen/dp_9"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_5"
                    android:layout_toStartOf="@+id/abnormal_value"
                    app:round_circleColor="#f1b36c"/>
                <androidx.appcompat.widget.AppCompatTextView
                    android:textSize="@dimen/sp_11"
                    android:textColor="@color/main_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/spo2_jujian2_value"
                    android:includeFontPadding="false"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_17"
                    android:layout_toStartOf="@+id/null_view"/>
                <com.jstyles.jcring.views.public_views.CircleColor
                    android:layout_width="@dimen/dp_9"
                    android:layout_height="@dimen/dp_9"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_5"
                    android:layout_toEndOf="@+id/normal_1"
                    app:round_circleColor="#a6f16c"/>
                <androidx.appcompat.widget.AppCompatTextView
                    android:textSize="@dimen/sp_11"
                    android:textColor="@color/main_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/spo2_jujian1_value"
                    android:includeFontPadding="false"
                    android:layout_centerVertical="true"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:background="@drawable/bg_rect_msg_item_r20_ripple"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_83"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginEnd="@dimen/dp_15"
                android:elevation="@dimen/dp_1">
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView
                        android:textSize="@dimen/sp_21"
                        android:textColor="@color/main_text_color"
                        android:id="@+id/day_avg_temp_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"/>
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:textColor="@color/msg_remark_text_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="@string/avg_string"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@color/home_detail_line_bg"
                    android:layout_width="@dimen/dp_0_5"
                    android:layout_height="@dimen/dp_40"/>
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView
                        android:textSize="@dimen/sp_21"
                        android:textColor="@color/main_text_color"
                        android:id="@+id/day_min_temp_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"/>
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:textColor="@color/msg_remark_text_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="@string/min_string"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@color/home_detail_line_bg"
                    android:layout_width="@dimen/dp_0_5"
                    android:layout_height="@dimen/dp_40"/>
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView
                        android:textSize="@dimen/sp_21"
                        android:textColor="@color/main_text_color"
                        android:id="@+id/day_max_temp_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"/>
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:textColor="@color/msg_remark_text_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="@string/max_string"/>
                </LinearLayout>
            </LinearLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:background="@drawable/bg_rect_msg_item_r20_ripple"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_55"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginEnd="@dimen/dp_15"
                android:elevation="@dimen/dp_1">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/main_text_color"
                    android:id="@+id/oxy_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/oxy_day_status"
                    android:layout_marginStart="@dimen/dp_15"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/oxy_status"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:textColor="@color/main_text_color"
                    android:id="@+id/oxy_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_15"
                    android:layout_marginEnd="@dimen/dp_26"
                    android:layout_alignParentEnd="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/oxy_tv"
                    app:layout_constraintTop_toTopOf="parent"/>
                <ImageView
                    android:id="@+id/oxy_iv"
                    android:visibility="invisible"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/lest_img"
                    android:adjustViewBounds="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_4"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_alignParentEnd="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/oxy_status"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <RelativeLayout
                android:id="@+id/day_avg_shoudong_rel"
                android:background="@drawable/bg_rect_msg_item_r20_ripple"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_55"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginBottom="@dimen/dp_10"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginEnd="@dimen/dp_15">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/main_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/manual_measurement_record"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_15"/>
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:textColor="@color/main_text_color"
                    android:id="@+id/day_avg_shoudong_tv_value"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="10次"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_26"
                    android:layout_alignParentEnd="true"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/lest_img"
                    android:adjustViewBounds="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_alignParentEnd="true"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/day_medSpo2_rel"
                android:background="@drawable/bg_rect_msg_item_r20_ripple"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_55"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginBottom="@dimen/dp_10"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginEnd="@dimen/dp_15">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/main_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/medical_grade_spo2_7"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_15"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/lest_img"
                    android:adjustViewBounds="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_alignParentEnd="true"/>
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>
</com.jstyles.jcring.views.ecg_stick_1791.MyNestedScrollView>
