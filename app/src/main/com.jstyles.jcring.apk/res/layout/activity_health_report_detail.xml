<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/main"
    android:background="@color/main_bg_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/layout_white_title"/>
    <androidx.core.widget.NestedScrollView
        android:orientation="vertical"
        android:scrollbars="none"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:fillViewport="true">
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <TextView
                android:textSize="@dimen/sp_36"
                android:textStyle="bold"
                android:textColor="@color/white"
                android:id="@+id/health_report_detail_tv_source_level"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_20"
                android:text="--"
                android:layout_marginStart="@dimen/dp_20"/>
            <TextView
                android:textSize="@dimen/sp_14"
                android:textColor="@color/msg_remark_text_color"
                android:id="@+id/health_report_detail_tv_source_remark"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:text="@string/health_all_pingfen_remark"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginEnd="@dimen/dp_27"/>
            <TextView
                android:textSize="@dimen/sp_14"
                android:textColor="@color/msg_remark_text_color"
                android:id="@+id/health_report_detail_tv_source_date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_10"
                android:text=""
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginEnd="@dimen/dp_27"/>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_15"
                android:layout_marginStart="@dimen/dp_20">
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/white"
                    android:gravity="start|center_vertical"
                    android:background="@mipmap/health_report_detail_remark_bg1"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp_27"
                    android:text="@string/health_report_detail_remark_title"
                    android:paddingStart="@dimen/dp_10"
                    android:paddingEnd="@dimen/dp_5"/>
                <ImageView
                    android:background="@mipmap/health_report_detail_remark_bg2"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp_27"
                    android:adjustViewBounds="true"/>
            </LinearLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginEnd="@dimen/dp_20">
                <ImageView
                    android:id="@+id/sleep_iv"
                    android:layout_width="@dimen/dp_25"
                    android:layout_height="@dimen/dp_25"
                    android:src="@mipmap/healthtab_2"
                    android:adjustViewBounds="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/sleep_tv"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/white"
                    android:id="@+id/sleep_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/health_report_detail_title1"
                    android:layout_marginStart="@dimen/dp_30"
                    app:layout_constraintBottom_toBottomOf="@+id/sleep_iv"
                    app:layout_constraintEnd_toStartOf="@+id/guideline"
                    app:layout_constraintStart_toEndOf="@+id/sleep_iv"
                    app:layout_constraintTop_toTopOf="@+id/sleep_iv"/>
                <androidx.constraintlayout.widget.Guideline
                    android:orientation="vertical"
                    android:id="@+id/guideline"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintGuide_percent="0.4"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/msg_remark_text_color"
                    android:id="@+id/health_report_detail_tv_sleep_count"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:layout_centerInParent="true"
                    android:layout_marginStart="@dimen/dp_30"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/health_report_detail_tv_sleep_level"
                    app:layout_constraintStart_toEndOf="@+id/guideline"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/white"
                    android:gravity="end"
                    android:id="@+id/health_report_detail_tv_sleep_level"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_20"
                    android:layout_alignParentEnd="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/health_report_detail_tv_sleep_count"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginEnd="@dimen/dp_20">
                <ImageView
                    android:id="@+id/excise_iv"
                    android:layout_width="@dimen/dp_25"
                    android:layout_height="@dimen/dp_25"
                    android:src="@mipmap/healthtab_0"
                    android:adjustViewBounds="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/excise_tv"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/white"
                    android:id="@+id/excise_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/health_report_detail_title2"
                    android:layout_marginStart="@dimen/dp_30"
                    app:layout_constraintBottom_toBottomOf="@+id/excise_iv"
                    app:layout_constraintEnd_toStartOf="@+id/guideline2"
                    app:layout_constraintStart_toEndOf="@+id/excise_iv"
                    app:layout_constraintTop_toTopOf="@+id/excise_iv"/>
                <androidx.constraintlayout.widget.Guideline
                    android:orientation="vertical"
                    android:id="@+id/guideline2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintGuide_percent="0.4"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/msg_remark_text_color"
                    android:id="@+id/health_report_detail_tv_activity_source"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:layout_centerInParent="true"
                    android:layout_marginStart="@dimen/dp_30"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/health_report_detail_tv_activity_status"
                    app:layout_constraintStart_toEndOf="@+id/guideline2"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/white"
                    android:gravity="end"
                    android:id="@+id/health_report_detail_tv_activity_status"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:layout_marginStart="@dimen/dp_20"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/health_report_detail_tv_activity_source"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginEnd="@dimen/dp_20">
                <ImageView
                    android:id="@+id/street_iv"
                    android:layout_width="@dimen/dp_25"
                    android:layout_height="@dimen/dp_25"
                    android:src="@mipmap/healthtab_8"
                    android:adjustViewBounds="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/street_tv"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/white"
                    android:id="@+id/street_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/health_report_detail_title3"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_30"
                    app:layout_constraintBottom_toBottomOf="@+id/street_iv"
                    app:layout_constraintEnd_toStartOf="@+id/guideline3"
                    app:layout_constraintStart_toEndOf="@+id/street_iv"
                    app:layout_constraintTop_toTopOf="@+id/street_iv"/>
                <androidx.constraintlayout.widget.Guideline
                    android:orientation="vertical"
                    android:id="@+id/guideline3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintGuide_percent="0.4"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/msg_remark_text_color"
                    android:id="@+id/health_report_detail_tv_stress_code"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:layout_centerInParent="true"
                    android:layout_marginStart="@dimen/dp_30"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/health_report_detail_tv_stress_status"
                    app:layout_constraintStart_toEndOf="@+id/guideline3"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/white"
                    android:gravity="end"
                    android:id="@+id/health_report_detail_tv_stress_status"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_20"
                    android:layout_alignParentEnd="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/health_report_detail_tv_stress_code"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginEnd="@dimen/dp_20">
                <ImageView
                    android:id="@+id/hr_iv"
                    android:layout_width="@dimen/dp_25"
                    android:layout_height="@dimen/dp_25"
                    android:src="@mipmap/healthtab_1"
                    android:adjustViewBounds="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/hr_tv"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/white"
                    android:id="@+id/hr_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/health_report_detail_title4"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_30"
                    app:layout_constraintBottom_toBottomOf="@+id/hr_iv"
                    app:layout_constraintEnd_toStartOf="@+id/guideline4"
                    app:layout_constraintStart_toEndOf="@+id/hr_iv"
                    app:layout_constraintTop_toTopOf="@+id/hr_iv"/>
                <androidx.constraintlayout.widget.Guideline
                    android:orientation="vertical"
                    android:id="@+id/guideline4"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintGuide_percent="0.4"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/msg_remark_text_color"
                    android:id="@+id/health_report_detail_tv_avg_hr"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:layout_centerInParent="true"
                    android:layout_marginStart="@dimen/dp_30"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/health_report_detail_tv_hr_status"
                    app:layout_constraintStart_toEndOf="@+id/guideline4"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/white"
                    android:gravity="end"
                    android:id="@+id/health_report_detail_tv_hr_status"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_20"
                    android:layout_alignParentEnd="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/health_report_detail_tv_avg_hr"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginEnd="@dimen/dp_20">
                <ImageView
                    android:id="@+id/sop_iv"
                    android:layout_width="@dimen/dp_25"
                    android:layout_height="@dimen/dp_25"
                    android:src="@mipmap/healthtab_3"
                    android:adjustViewBounds="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/sop_tv"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/white"
                    android:id="@+id/sop_tv"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="@string/health_report_detail_title5"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_30"
                    app:layout_constraintBottom_toBottomOf="@+id/sop_iv"
                    app:layout_constraintEnd_toStartOf="@+id/guideline5"
                    app:layout_constraintStart_toEndOf="@+id/sop_iv"
                    app:layout_constraintTop_toTopOf="@+id/sop_iv"/>
                <androidx.constraintlayout.widget.Guideline
                    android:orientation="vertical"
                    android:id="@+id/guideline5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    app:layout_constraintGuide_percent="0.4"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/msg_remark_text_color"
                    android:id="@+id/health_report_detail_tv_avg_spo2"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:layout_centerInParent="true"
                    android:layout_marginStart="@dimen/dp_30"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toStartOf="@+id/health_report_detail_tv_spo2_status"
                    app:layout_constraintStart_toEndOf="@+id/guideline5"
                    app:layout_constraintTop_toTopOf="parent"/>
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/white"
                    android:gravity="end"
                    android:id="@+id/health_report_detail_tv_spo2_status"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="--"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_20"
                    android:layout_alignParentEnd="true"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toEndOf="@+id/health_report_detail_tv_avg_spo2"
                    app:layout_constraintTop_toTopOf="parent"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <LinearLayout
                android:background="@color/white30"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_1_5"
                android:layout_margin="@dimen/dp_20"/>
            <TextView
                android:textSize="@dimen/sp_14"
                android:textColor="@color/msg_remark_text_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/health_report_detail_data_remark"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginEnd="@dimen/dp_20"/>
        </LinearLayout>
    </androidx.core.widget.NestedScrollView>
</LinearLayout>
