<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/devices_unbind"
    android:background="@drawable/bleerror_backmian"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingStart="@dimen/dp_15"
        android:paddingEnd="@dimen/dp_15"/>
    <ImageView
        android:id="@+id/errortips_2206"
        android:padding="@dimen/dp_5"
        android:layout_width="@dimen/dp_27"
        android:layout_height="@dimen/dp_27"
        android:src="@mipmap/errortips_2206"
        android:layout_centerVertical="true"
        android:layout_marginStart="@dimen/dp_5"/>
    <ImageView
        android:id="@+id/errorcancle_2206"
        android:background="@drawable/ripple_bg"
        android:padding="@dimen/dp_5"
        android:visibility="gone"
        android:layout_width="@dimen/dp_22"
        android:layout_height="@dimen/dp_22"
        android:src="@mipmap/errorcancle_2206"
        android:layout_centerVertical="true"
        android:layout_marginEnd="@dimen/dp_5"
        android:layout_alignParentEnd="true"/>
    <TextView
        android:textSize="@dimen/sp_11"
        android:textColor="@android:color/white"
        android:id="@+id/Available_devices_tip5"
        android:background="@drawable/ripple_bg"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginBottom="@dimen/dp_10"
        android:includeFontPadding="false"
        android:layout_marginStart="@dimen/dp_7"
        android:layout_marginEnd="@dimen/dp_7"
        android:layout_toStartOf="@+id/errorcancle_2206"
        android:layout_toEndOf="@+id/errortips_2206"/>
</RelativeLayout>
