<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="@color/main_bg_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:keepScreenOn="true">
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/RadioGroup_rt_me"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_45"
        app:layout_collapseMode="pin"
        app:layout_scrollFlags="scroll">
        <com.google.android.flexbox.FlexboxLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:alignItems="center"
            app:flexWrap="nowrap"
            app:justifyContent="space_between">
            <RelativeLayout
                android:id="@+id/me_info_rt"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:elevation="@dimen/dp_1"
                android:paddingHorizontal="@dimen/dp_15"
                android:paddingVertical="@dimen/dp_10">
                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/user_img_me"
                    android:layout_width="@dimen/dp_30"
                    android:layout_height="@dimen/dp_30"
                    android:src="@drawable/ic_user_img"
                    android:scaleType="fitXY"
                    android:layout_centerVertical="true"/>
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:textColor="@color/main_text_color"
                    android:id="@+id/user_phone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="账号名称"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_10"
                    android:layout_toEndOf="@+id/user_img_me"
                    android:textFontWeight="600"/>
            </RelativeLayout>
            <RelativeLayout
                android:id="@+id/me_item_un_bind"
                android:background="@drawable/bg_769df4_r4_ripple"
                android:paddingLeft="@dimen/dp_10"
                android:paddingRight="@dimen/dp_10"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_25"
                android:layout_marginEnd="@dimen/dp_20"
                android:layout_alignParentEnd="true">
                <TextView
                    android:textSize="@dimen/sp_11"
                    android:textColor="#ffffff"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/devices_unbind"
                    android:layout_centerInParent="true"/>
            </RelativeLayout>
        </com.google.android.flexbox.FlexboxLayout>
        <RelativeLayout
            android:id="@+id/layout_unbind"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_5">
            <LinearLayout
                android:orientation="vertical"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_20">
                <TextView
                    android:textSize="@dimen/sp_16"
                    android:textColor="@color/white"
                    android:id="@+id/me_tv_ring_no_name"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/no_bind_ring"/>
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:textColor="@color/white"
                    android:gravity="center"
                    android:id="@+id/me_item_tv_start"
                    android:background="@drawable/bg_769df4_r4_ripple"
                    android:paddingLeft="@dimen/dp_15"
                    android:paddingRight="@dimen/dp_15"
                    android:layout_width="wrap_content"
                    android:layout_height="@dimen/dp_28"
                    android:layout_marginTop="@dimen/dp_15"
                    android:text="@string/bangding_satrt"/>
            </LinearLayout>
            <ImageView
                android:id="@+id/me_tv_ring_no_bind_icon"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_101"
                android:src="@mipmap/me_ring_icon"
                android:adjustViewBounds="true"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_38"
                android:layout_alignParentEnd="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/me_have_ring_rel"
            android:background="@drawable/bg_rect_msg_item_r20_v2"
            android:visibility="gone"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_5"
            android:layout_below="@+id/me_title_remark"
            android:elevation="@dimen/dp_1"
            android:paddingVertical="@dimen/dp_6">
            <ImageView
                android:id="@+id/me_have_ring_iv"
                android:layout_width="@dimen/dp_80"
                android:layout_height="@dimen/dp_80"
                android:layout_marginLeft="@dimen/dp_15"
                android:src="@mipmap/ring_icon"
                android:adjustViewBounds="true"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="@dimen/sp_16"
                android:textColor="@color/main_text_color"
                android:id="@+id/me_tv_ring_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_7"
                android:text="@string/no_bind_ring"
                android:layout_alignTop="@+id/me_have_ring_iv"
                android:layout_marginStart="@dimen/dp_12"
                android:layout_toEndOf="@+id/me_have_ring_iv"/>
