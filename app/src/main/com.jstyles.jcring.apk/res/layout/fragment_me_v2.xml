                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/msg_remark_text_color"
                    android:id="@+id/home_tv_quantity"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:text="--"
                    android:layout_marginStart="@dimen/dp_5"/>
                <ImageView
                    android:id="@+id/home_iv_device_status"
                    android:layout_width="@dimen/dp_9"
                    android:layout_height="@dimen/dp_9"
                    android:src="@drawable/cicle_red"
                    android:layout_marginStart="@dimen/dp_20"/>
                <TextView
                    android:textColor="@color/msg_remark_text_color"
                    android:id="@+id/home_tv_device_status"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/device_disconnected"
                    android:layout_marginStart="@dimen/dp_7_5"/>
            </LinearLayout>
            <LinearLayout
                android:gravity="center"
                android:orientation="horizontal"
                android:id="@+id/layout_device_version"
                android:visibility="gone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/layout_device_state"
                android:layout_alignLeft="@+id/layout_device_state"
                android:paddingVertical="@dimen/dp_10">
                <ImageView
                    android:layout_width="@dimen/dp_16"
                    android:layout_height="@dimen/dp_16"
                    android:src="@drawable/ic_device_version_tag"/>
                <TextView
                    android:textSize="@dimen/sp_13"
                    android:textColor="@color/whites"
                    android:id="@+id/tv_device_version"
                    android:paddingLeft="@dimen/dp_5"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:includeFontPadding="false"/>
                <LinearLayout
                    android:id="@+id/layout_update"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/dp_10">
                    <ImageView
