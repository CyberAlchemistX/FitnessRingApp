                    android:id="@+id/layout_member_text"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_9_5">
                    <ImageView
                        android:id="@+id/image_member_left"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_2"
                        android:src="@mipmap/ic_member_left"
                        android:layout_marginStart="@dimen/dp_11_5"/>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/dp_10">
                        <TextView
                            android:textSize="@dimen/sp_18"
                            android:textStyle="bold"
                            android:textColor="@color/main_text_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/member_health"/>
                        <TextView
                            android:textSize="@dimen/sp_14"
                            android:textColor="@color/msg_remark_text_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_10"
                            android:text="@string/chengyuan_mamager"
                            android:includeFontPadding="false"/>
                    </LinearLayout>
                </LinearLayout>
            </RelativeLayout>
            <RelativeLayout
