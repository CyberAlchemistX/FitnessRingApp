                android:background="@mipmap/bg_emotion"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_125"
                android:layout_marginTop="@dimen/dp_10"
                android:paddingVertical="@dimen/dp_20">
                <LinearLayout
                    android:orientation="horizontal"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/dp_9_5">
                    <ImageView
                        android:id="@+id/bg_emotion_img"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_2"
                        android:src="@mipmap/bg_emotion_img"
                        android:layout_marginStart="@dimen/dp_11_5"/>
                    <LinearLayout
                        android:orientation="vertical"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/dp_10">
                        <TextView
                            android:textSize="@dimen/sp_18"
                            android:textStyle="bold"
                            android:textColor="@color/main_text_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="@string/emotion"/>
                        <TextView
                            android:textSize="@dimen/sp_14"
                            android:textColor="@color/msg_remark_text_color"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginTop="@dimen/dp_10"
                            android:text="@string/emotion1"
                            android:includeFontPadding="false"/>
                    </LinearLayout>
                </LinearLayout>
