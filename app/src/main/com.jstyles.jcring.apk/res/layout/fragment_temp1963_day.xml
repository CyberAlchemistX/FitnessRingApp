<?xml version="1.0" encoding="utf-8"?>
<com.jstyles.jcring.views.ecg_stick_1791.MyNestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/nestedScrollView"
    android:background="@color/main_bg_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:keepScreenOn="true">
        <RelativeLayout
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_41"
            android:layout_marginTop="@dimen/dp_20_5">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/last_img"
                android:background="@drawable/ripple_bg"
                android:padding="@dimen/dp_9"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_31"
                android:src="@mipmap/last_img_fill"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_27"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/nest_img"
                android:background="@drawable/ripple_bg"
                android:padding="@dimen/dp_9"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_31"
                android:src="@mipmap/next_img_fill"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_27"
                android:layout_alignParentEnd="true"/>
            <TextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:id="@+id/date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <com.jstyles.jcring.views.FontMiSansMediumView
            android:textSize="@dimen/sp_23"
            android:textColor="@color/main_text_color"
            android:gravity="center"
            android:id="@+id/TEMP_value"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="0"
            android:includeFontPadding="false"
            android:layout_below="@+id/title"
            android:layout_centerHorizontal="true"/>
        <TextView
            android:textSize="@dimen/sp_10"
            android:textColor="@color/msg_remark_text_color"
            android:gravity="center"
            android:id="@+id/heart_value_tips"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="@dimen/dp_13"
            android:text="@string/sehshidu"
            android:includeFontPadding="false"
            android:layout_alignBottom="@+id/TEMP_value"
            android:layout_marginStart="@dimen/dp_5"
            android:layout_toEndOf="@+id/TEMP_value"/>
        <RelativeLayout
            android:id="@+id/index_date_rt"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_below="@+id/heart_value_tips">
            <com.jstyles.jcring.views.public_views.MultiplTextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:gravity="center"
                android:id="@+id/index_date"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="--"
                android:includeFontPadding="false"
                android:layout_centerInParent="true"
                app:removeDefaultPadding="true"/>
        </RelativeLayout>
        <com.jstyles.jcring.views.temp.TempDayView
            android:id="@+id/temp_dayview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_260"
            android:layout_marginTop="@dimen/dp_7"
            android:layout_below="@+id/index_date_rt"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/temp_dayview">
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:background="@drawable/bg_rect_msg_item_r20_ripple"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_83"
                android:layout_marginTop="@dimen/dp_30"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginEnd="@dimen/dp_15"
                android:elevation="@dimen/dp_1">
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:visibility="gone"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView
                        android:textSize="@dimen/sp_21"
                        android:textColor="@color/main_text_color"
                        android:id="@+id/day_avg_temp_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="--"/>
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:textColor="@color/msg_remark_text_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="@string/day_avg_temp"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@color/home_detail_line_bg"
                    android:visibility="gone"
                    android:layout_width="@dimen/dp_0_5"
                    android:layout_height="@dimen/dp_40"/>
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <LinearLayout
                        android:gravity="bottom|center_horizontal"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:textSize="@dimen/sp_21"
                            android:textColor="@color/main_text_color"
                            android:id="@+id/day_max_temp_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="--"/>
                        <TextView
                            android:textSize="@dimen/sp_10"
                            android:textColor="@color/msg_remark_text_color"
                            android:id="@+id/day_max_temp_unit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="â„ƒ"
                            android:layout_marginStart="@dimen/dp_5"/>
                    </LinearLayout>
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:textColor="@color/msg_remark_text_color"
                        android:gravity="center"
                        android:id="@+id/day_max_temp_remark_unit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="@string/unit_above_baseline_avg"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@color/home_detail_line_bg"
                    android:layout_width="@dimen/dp_0_5"
                    android:layout_height="@dimen/dp_40"/>
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <LinearLayout
                        android:gravity="bottom|center_horizontal"
                        android:orientation="horizontal"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content">
                        <TextView
                            android:textSize="@dimen/sp_21"
                            android:textColor="@color/main_text_color"
                            android:id="@+id/day_min_temp_value"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="--"/>
                        <TextView
                            android:textSize="@dimen/sp_10"
                            android:textColor="@color/msg_remark_text_color"
                            android:id="@+id/day_min_temp_unit"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="â„ƒ"
                            android:layout_marginStart="@dimen/dp_5"/>
                    </LinearLayout>
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:textColor="@color/msg_remark_text_color"
                        android:gravity="center"
                        android:id="@+id/day_min_temp_remark_unit"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="@string/unit_diyu_baseline_avg"/>
                </LinearLayout>
            </LinearLayout>
            <RelativeLayout
                android:id="@+id/day_avg_shoudong_rel"
                android:background="@drawable/bg_rect_msg_item_r20_ripple"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_55"
                android:layout_marginTop="@dimen/dp_10"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginEnd="@dimen/dp_15">
                <TextView
                    android:textSize="@dimen/sp_15"
                    android:textColor="@color/main_text_color"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/manual_measurement_record"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="@dimen/dp_15"/>
                <TextView
                    android:textSize="@dimen/sp_12"
                    android:textColor="@color/main_text_color"
                    android:id="@+id/wearing_ci"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="10æ¬¡"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_26"
                    android:layout_alignParentEnd="true"/>
                <ImageView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:src="@mipmap/lest_img"
                    android:adjustViewBounds="true"
                    android:layout_centerVertical="true"
                    android:layout_marginEnd="@dimen/dp_15"
                    android:layout_alignParentEnd="true"/>
            </RelativeLayout>
        </LinearLayout>
    </RelativeLayout>
</com.jstyles.jcring.views.ecg_stick_1791.MyNestedScrollView>
