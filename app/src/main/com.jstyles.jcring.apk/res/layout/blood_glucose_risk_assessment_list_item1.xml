<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:gravity="center"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
    <LinearLayout
        android:orientation="vertical"
        android:id="@+id/blood_glucose_risk_assessment_list_item_lin"
        android:background="@drawable/bg_rect_msg_item_r20"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/dp_10"
        android:paddingStart="@dimen/dp_20"
        android:paddingEnd="@dimen/dp_20"
        android:layout_marginStart="@dimen/dp_15"
        android:layout_marginEnd="@dimen/dp_15">
        <TextView
            android:textSize="@dimen/sp_16"
            android:textColor="@color/main_text_color"
            android:id="@+id/blood_glucose_risk_assessment_list_item1_tv_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_20"
            android:text="Jan 20, 2024"/>
        <LinearLayout
            android:background="@color/white30"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_1"
            android:layout_marginTop="@dimen/dp_15"/>
        <androidx.recyclerview.widget.RecyclerView
            android:id="@+id/blood_glucose_risk_assessment_list_item1_rv_data"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_marginBottom="@dimen/dp_20"/>
    </LinearLayout>
</LinearLayout>
