<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:id="@+id/main"
    android:background="@color/main_bg_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <include layout="@layout/layout_white_title"/>
    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_above="@+id/save"
        android:layout_below="@+id/rl_title">
        <TextView
            android:textSize="@dimen/sp_12"
            android:textColor="@color/msg_remark_text_color"
            android:id="@+id/Menstrual_infoset_tips1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="@dimen/dp_25"
            android:text="@string/womenhelath_mode_set_tips"
            android:includeFontPadding="false"
            android:layout_marginStart="@dimen/dp_25"/>
        <RelativeLayout
            android:id="@+id/period_rt"
            android:background="@drawable/bg_rect_msg_item_r20_ripple"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_85"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_below="@+id/Menstrual_infoset_tips1"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginEnd="@dimen/dp_10">
            <View
                android:id="@+id/null1"
                android:layout_width="@dimen/dp_0"
                android:layout_height="@dimen/dp_0"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="@dimen/sp_15"
                android:textColor="@color/main_text_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_4"
                android:text="@string/period_only"
                android:includeFontPadding="false"
                android:layout_above="@+id/null1"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_20"/>
            <TextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_4"
                android:text="@string/period_only_tips"
                android:includeFontPadding="false"
                android:layout_below="@+id/null1"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginEnd="@dimen/dp_50"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/lest_img1"
                android:layout_width="@dimen/dp_8"
                android:layout_height="@dimen/dp_11"
                android:src="@mipmap/lest_img"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_15"
                android:layout_alignParentEnd="true"/>
            <TextView
                android:textSize="@dimen/sp_12"
                android:textColor="#898989"
                android:id="@+id/startdateperiod_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_5"
                android:layout_toStartOf="@+id/lest_img1"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/pregnancy_rt"
            android:background="@drawable/bg_rect_msg_item_r20_ripple"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_85"
            android:layout_marginTop="@dimen/dp_15"
            android:layout_below="@+id/period_rt"
            android:layout_marginStart="@dimen/dp_10"
            android:layout_marginEnd="@dimen/dp_10">
            <View
                android:id="@+id/null2"
                android:layout_width="@dimen/dp_0"
                android:layout_height="@dimen/dp_0"
                android:layout_centerVertical="true"/>
            <TextView
                android:textSize="@dimen/sp_15"
                android:textColor="@color/main_text_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_4"
                android:text="@string/pregnancy_only"
                android:includeFontPadding="false"
                android:layout_above="@+id/null2"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_20"/>
            <TextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_4"
                android:text="@string/pregnancy_only_tips"
                android:includeFontPadding="false"
                android:layout_below="@+id/null2"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_20"
                android:layout_marginEnd="@dimen/dp_50"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/lest_img2"
                android:layout_width="@dimen/dp_8"
                android:layout_height="@dimen/dp_11"
                android:src="@mipmap/lest_img"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_15"
                android:layout_alignParentEnd="true"/>
            <TextView
                android:textSize="@dimen/sp_12"
                android:textColor="#898989"
                android:id="@+id/lengthperiod"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_5"
                android:layout_toStartOf="@+id/lest_img2"/>
        </RelativeLayout>
    </RelativeLayout>
</RelativeLayout>
