<?xml version="1.0" encoding="utf-8"?>
<com.jstyles.jcring.views.ecg_stick_1791.MyNestedScrollView xmlns:android="http://schemas.android.com/apk/res/android" xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:id="@+id/nestedScrollView"
    android:background="@color/main_bg_color"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:fillViewport="true">
    <RelativeLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:keepScreenOn="true">
        <RelativeLayout
            android:id="@+id/title"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/dp_41"
            android:layout_marginTop="@dimen/dp_20_5">
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/last_img"
                android:background="@drawable/ripple_bg"
                android:padding="@dimen/dp_9"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_31"
                android:src="@mipmap/last_img_fill"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_marginStart="@dimen/dp_27"/>
            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/nest_img"
                android:background="@drawable/ripple_bg"
                android:padding="@dimen/dp_9"
                android:layout_width="@dimen/dp_30"
                android:layout_height="@dimen/dp_31"
                android:src="@mipmap/next_img_fill"
                android:scaleType="fitXY"
                android:layout_centerVertical="true"
                android:layout_marginEnd="@dimen/dp_27"
                android:layout_alignParentEnd="true"/>
            <TextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:id="@+id/date"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:includeFontPadding="false"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <LinearLayout
            android:gravity="center"
            android:orientation="horizontal"
            android:id="@+id/title_rt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/title">
            <com.jstyles.jcring.views.FontMiSansMediumView
                android:textSize="@dimen/sp_23"
                android:textColor="@color/main_text_color"
                android:gravity="center"
                android:id="@+id/TEMP_value"
                android:layout_width="wrap_content"
                android:layout_height="@dimen/dp_40"
                android:text="0"/>
            <TextView
                android:textSize="@dimen/sp_10"
                android:textColor="@color/msg_remark_text_color"
                android:gravity="bottom"
                android:id="@+id/heart_value_tips"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:layout_marginBottom="@dimen/dp_5"
                android:text="@string/ms"
                android:includeFontPadding="false"
                android:layout_marginStart="@dimen/dp_3"/>
        </LinearLayout>
        <RelativeLayout
            android:id="@+id/heart_value_tips_rt"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_15"
            android:layout_below="@+id/title_rt">
            <TextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:id="@+id/status_value"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="--"
                android:includeFontPadding="false"
                android:layout_centerInParent="true"/>
        </RelativeLayout>
        <RelativeLayout
            android:id="@+id/index_date_rt"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_10"
            android:layout_marginTop="@dimen/dp_10"
            android:layout_below="@+id/heart_value_tips_rt">
            <com.jstyles.jcring.views.public_views.MultiplTextView
                android:textSize="@dimen/sp_12"
                android:textColor="@color/msg_remark_text_color"
                android:gravity="center"
                android:id="@+id/index_date"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"
                android:text="00"
                android:includeFontPadding="false"
                android:layout_centerInParent="true"
                android:layout_centerHorizontal="true"
                app:removeDefaultPadding="true"/>
        </RelativeLayout>
        <com.jstyles.jcring.views.oximeter_1963.HRV_dayView
            android:id="@+id/temp_dayview"
            android:layout_width="match_parent"
            android:layout_height="@dimen/dp_260"
            android:layout_marginTop="@dimen/dp_7"
            android:layout_below="@+id/index_date_rt"/>
        <LinearLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/temp_dayview"
            android:layout_alignParentBottom="true">
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/dp_15"
                android:paddingStart="@dimen/dp_30">
                <RelativeLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <androidx.appcompat.widget.AppCompatTextView
                        android:textSize="@dimen/sp_10"
                        android:textColor="@color/main_text_color"
                        android:gravity="end"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/low_side_tips"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/dp_8"
                        android:layout_toStartOf="@+id/chat_stress_lv1"/>
                    <com.jstyles.jcring.views.public_views.CircleColor
                        android:id="@+id/chat_stress_lv1"
                        android:layout_width="@dimen/dp_9"
                        android:layout_height="@dimen/dp_9"
                        android:layout_centerInParent="true"
                        app:round_circleColor="@color/chat_hrv_lv1"/>
                    <androidx.appcompat.widget.AppCompatTextView
                        android:textSize="@dimen/sp_11"
                        android:textColor="@color/main_text_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="&lt;25"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_toEndOf="@+id/chat_stress_lv1"/>
                </RelativeLayout>
                <RelativeLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <androidx.appcompat.widget.AppCompatTextView
                        android:textSize="@dimen/sp_10"
                        android:textColor="@color/main_text_color"
                        android:gravity="end"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/normal_1"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/dp_8"
                        android:layout_toStartOf="@+id/chat_stress_lv2"/>
                    <com.jstyles.jcring.views.public_views.CircleColor
                        android:id="@+id/chat_stress_lv2"
                        android:layout_width="@dimen/dp_9"
                        android:layout_height="@dimen/dp_9"
                        android:layout_centerInParent="true"
                        app:round_circleColor="@color/chat_hrv_lv2"/>
                    <androidx.appcompat.widget.AppCompatTextView
                        android:textSize="@dimen/sp_11"
                        android:textColor="@color/main_text_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="25-50"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_toEndOf="@+id/chat_stress_lv2"/>
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:orientation="horizontal"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/dp_15"
                android:paddingStart="@dimen/dp_30">
                <RelativeLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <androidx.appcompat.widget.AppCompatTextView
                        android:textSize="@dimen/sp_10"
                        android:textColor="@color/main_text_color"
                        android:gravity="end"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/good_tips"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/dp_8"
                        android:layout_toStartOf="@+id/chat_stress_lv3"/>
                    <com.jstyles.jcring.views.public_views.CircleColor
                        android:id="@+id/chat_stress_lv3"
                        android:layout_width="@dimen/dp_9"
                        android:layout_height="@dimen/dp_9"
                        android:layout_centerInParent="true"
                        app:round_circleColor="@color/chat_hrv_lv3"/>
                    <androidx.appcompat.widget.AppCompatTextView
                        android:textSize="@dimen/sp_11"
                        android:textColor="@color/main_text_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="51-75"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_toEndOf="@+id/chat_stress_lv3"/>
                </RelativeLayout>
                <RelativeLayout
                    android:gravity="center_vertical"
                    android:orientation="horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="1">
                    <androidx.appcompat.widget.AppCompatTextView
                        android:textSize="@dimen/sp_10"
                        android:textColor="@color/main_text_color"
                        android:gravity="end"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/excellent_tips"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/dp_8"
                        android:layout_toStartOf="@+id/chat_stress_lv4"/>
                    <com.jstyles.jcring.views.public_views.CircleColor
                        android:id="@+id/chat_stress_lv4"
                        android:layout_width="@dimen/dp_9"
                        android:layout_height="@dimen/dp_9"
                        android:layout_centerInParent="true"
                        app:round_circleColor="@color/chat_hrv_lv4"/>
                    <androidx.appcompat.widget.AppCompatTextView
                        android:textSize="@dimen/sp_11"
                        android:textColor="@color/main_text_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="&gt;75"
                        android:layout_centerVertical="true"
                        android:layout_marginStart="@dimen/dp_5"
                        android:layout_toEndOf="@+id/chat_stress_lv4"/>
                </RelativeLayout>
            </LinearLayout>
            <LinearLayout
                android:gravity="center_vertical"
                android:orientation="horizontal"
                android:background="@drawable/bg_rect_msg_item_r20_ripple"
                android:layout_width="match_parent"
                android:layout_height="@dimen/dp_83"
                android:layout_marginBottom="@dimen/dp_10"
                android:layout_marginStart="@dimen/dp_15"
                android:layout_marginEnd="@dimen/dp_15"
                android:elevation="@dimen/dp_1">
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView
                        android:textSize="@dimen/sp_21"
                        android:textColor="@color/main_text_color"
                        android:id="@+id/day_avg_temp_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"/>
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:textColor="@color/msg_remark_text_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="@string/avg_string"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@color/home_detail_line_bg"
                    android:layout_width="@dimen/dp_0_5"
                    android:layout_height="@dimen/dp_40"/>
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView
                        android:textSize="@dimen/sp_21"
                        android:textColor="@color/main_text_color"
                        android:id="@+id/day_min_temp_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"/>
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:textColor="@color/msg_remark_text_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="@string/min_string"/>
                </LinearLayout>
                <LinearLayout
                    android:background="@color/home_detail_line_bg"
                    android:layout_width="@dimen/dp_0_5"
                    android:layout_height="@dimen/dp_40"/>
                <LinearLayout
                    android:gravity="center"
                    android:orientation="vertical"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="1">
                    <TextView
                        android:textSize="@dimen/sp_21"
                        android:textColor="@color/main_text_color"
                        android:id="@+id/day_max_temp_value"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"/>
                    <TextView
                        android:textSize="@dimen/sp_12"
                        android:textColor="@color/msg_remark_text_color"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginTop="@dimen/dp_10"
                        android:text="@string/max_string"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </RelativeLayout>
</com.jstyles.jcring.views.ecg_stick_1791.MyNestedScrollView>
