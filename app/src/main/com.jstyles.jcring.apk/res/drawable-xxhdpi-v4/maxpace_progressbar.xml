<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:id="@android:id/background">
        <shape>
            <corners
                android:topLeftRadius="@dimen/dp_10"
                android:bottomLeftRadius="@dimen/dp_10"/>
            <solid android:color="#54efefef"/>
        </shape>
    </item>
    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/dp_10"/>
                <solid android:color="#e9603c"/>
            </shape>
        </clip>
    </item>
    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners
                    android:topRightRadius="@dimen/dp_10"
                    android:bottomRightRadius="@dimen/dp_10"/>
                <solid android:color="#ce2235"/>
            </shape>
        </clip>
    </item>
</layer-list>
